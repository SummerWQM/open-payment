<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ChannelDao">
    <select id="getChannels" resultType="com.entity.Channel">
    select * from payment_channel limit 3
  </select>
    <select id="getPaymentChannel" resultType="com.entity.Channel">
          select * from payment_channel where code = #{code}
    </select>
</mapper>